# ChromaDB URL (Docker container)
CHROMA_URL=http://localhost:8000

# Server Port
PORT=3001

# Collection name for embeddings
COLLECTION_NAME=ingres_groundwater

# PostgreSQL Configuration (Docker container)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=ingres

GROQ_API_KEY=your_groq_api_key_here

# Gemini API Key - Get from Google Cloud Console
GOOGLE_API_KEY=your_google_api_key_here

# LangSmith and LangChain Tracing Configuration
LANGSMITH_TRACING=true
LANGCHAIN_TRACING=true
LANGCHAIN_TRACING_V2=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT="Workspace 1"
