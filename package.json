{
  "name": "@sih2025/backend",
  "version": "1.0.0",
  "description": "RAG backend for INGRES groundwater data with local embeddings",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "ingest": "tsx src/scripts/ingest.ts",
    "load:data": "tsx src/scripts/loadData.ts",
    "db:seed": "tsx src/scripts/seedGroundwaterData.ts",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^1.1.4",
    "@langchain/google-genai": "^2.0.4",
    "@langchain/groq": "^1.0.2",
    "@langchain/langgraph": "^1.0.4",
    "@xenova/transformers": "^2.17.2",
    "chromadb": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.45.0",
    "express": "^4.18.2",
    "fuse.js": "^7.1.0",
    "groq-sdk": "^0.37.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.7",
    "zod": "^4.1.13",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.6",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
